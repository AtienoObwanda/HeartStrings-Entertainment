// Video availability change:
 const handleVideoAvailableChange = (event, index) => {
  const { name, value } = event.target;
  const updatedVideoAvailable = [...video_available];
  updatedVideoAvailable[0][name] = value;
  setvideo_available(updatedVideoAvailable);
};


// Price:
const handlePriceChange = (event) => {
  const { name, value } = event.target;
  const updatedVideoAvailable = [{ ...video_available[0], [name]: value }];
  setVideoAvailable(updatedVideoAvailable);
};
